{
	"name": "@mingling/mobile",
	"version": "1.0.0",
	"description": "Mingling mobile app",
	"main": "index.ts",
	"private": true,
	"scripts": {
		"submit": "eas submit --platform ios",
		"version:patch": "npm version patch --no-git-tag-version",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"local": "cross-env APP_ENV=local expo start",
		"dev": "cross-env APP_ENV=development expo start",
		"qa": "cross-env APP_ENV=qa expo start",
		"prod": "cross-env APP_ENV=production expo start",
		"build:dev": "run-s version:patch build:dev:ios build:dev:android",
		"build:qa": "run-s version:patch build:qa:ios build:dev:android",
		"build:prod:all": "run-s version:patch build:prod:ios build:prod:android",
		"prebuild:dev": "cross-env APP_ENV=development expo prebuild && node ../../scripts/prebuild.cjs mobile",
		"prebuild:qa": "cross-env APP_ENV=qa expo prebuild && node ../../scripts/prebuild.cjs mobile",
		"prebuild:prod": "cross-env APP_ENV=production expo prebuild && node ../../scripts/prebuild.cjs mobile",
		"build:dev:ios": "cd ios && pod install && bundle exec fastlane ios build_development",
		"build:qa:ios": "cd ios && pod install && bundle exec fastlane ios beta",
		"build:prod:ios": "cd ios && pod install && bundle exec fastlane ios build",
		"build:dev:android": "cd android && bundle exec fastlane android build_development",
		"build:prod:android": "cd android && bundle exec fastlane android build_and_upload",
		"bundle:ios": "npx expo export --platform ios",
		"bundle:android": "npx expo export --platform android",
		"build:eas:dev:ios": "eas build -p ios --profile development",
		"build:eas:qa:ios": "eas build -p ios --profile qa --auto-submit",
		"build:eas:prod:ios": "eas build -p ios --profile production",
		"build:eas:preview:ios": "eas build -p ios --profile preview",
		"build:local": "eas build -p ios --local",
		"lint": "eslint -c ./.eslintrc.cjs --ignore-path ./.eslintignore \"./**/*.{ts,tsx,cjs}\"",
		"type-check": "tsc --project ./tsconfig.json"
	},
	"dependencies": {
		"@config-plugins/react-native-webrtc": "9.0.0",
		"@expo/config": "10.0.5",
		"@expo/metro-config": "0.19.4",
		"@expo/vector-icons": "14.0.4",
		"@gorhom/bottom-sheet": "5.0.6",
		"@mingling/types": "workspace:*",
		"@react-native-async-storage/async-storage": "1.23.1",
		"@react-native-community/netinfo": "11.4.1",
		"@react-native-masked-view/masked-view": "0.3.2",
		"@react-navigation/drawer": "^7.0.0",
		"babel-preset-expo": "12.0.2",
		"clsx": "2.1.1",
		"dayjs": "1.11.13",
		"dotenv": "16.4.5",
		"expo": "~52.0.11",
		"expo-asset": "~11.0.1",
		"expo-build-properties": "~0.13.1",
		"expo-checkbox": "~4.0.0",
		"expo-constants": "~17.0.3",
		"expo-dev-client": "~5.0.4",
		"expo-device": "~7.0.1",
		"expo-font": "~13.0.1",
		"expo-haptics": "~14.0.0",
		"expo-image": "~2.0.2",
		"expo-image-picker": "~16.0.3",
		"expo-linear-gradient": "~14.0.1",
		"expo-linking": "~7.0.3",
		"expo-localization": "~16.0.0",
		"expo-notifications": "~0.29.8",
		"expo-router": "~4.0.9",
		"expo-secure-store": "~14.0.0",
		"expo-splash-screen": "~0.29.13",
		"lottie-react-native": "7.1.0",
		"metro-cache": "0.80.9",
		"nativewind": "2.0.11",
		"react": "18.3.1",
		"react-native": "0.76.3",
		"react-native-gesture-handler": "~2.20.2",
		"react-native-reanimated": "~3.16.3",
		"react-native-safe-area-context": "4.12.0",
		"react-native-screens": "4.1.0",
		"react-native-svg": "15.8.0",
		"react-native-vector-icons": "10.2.0",
		"react-native-webrtc": "124.0.4",
		"socket.io-client": "4.8.1",
		"tailwind-merge": "2.5.4",
		"zustand": "5.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.24.5",
		"@types/node": "20.14.1",
		"@types/react": "18.3.12",
		"@types/react-native-vector-icons": "6.4.18",
		"@typescript-eslint/eslint-plugin": "6.18.1",
		"@typescript-eslint/parser": "6.18.1",
		"cross-env": "7.0.3",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-deprecation": "1.5.0",
		"eslint-plugin-i18next": "6.0.3",
		"eslint-plugin-import": "2.29.1",
		"eslint-plugin-jsx-a11y": "6.8.0",
		"eslint-plugin-react": "7.32.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-unicorn": "47.0.0",
		"eslint-plugin-unused-imports": "3.2.0",
		"reactotron-plugin-zustand": "2.0.3",
		"reactotron-react-native": "5.1.7",
		"tailwindcss": "3.3.2",
		"ts-mock-api": "1.0.13",
		"typescript": "5.6.3"
	},
	"peerDependencies": {
		"@types/node": "20.14.1",
		"@typescript-eslint/parser": "6.18.1",
		"eslint": "8.57.0",
		"npm-run-all2": "6.2.6",
		"prettier": "3.3.2",
		"react-i18next": "14.1.3"
	}
}
