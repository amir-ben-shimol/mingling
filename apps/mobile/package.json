{
	"name": "@mingling/mobile",
	"version": "1.0.0",
	"description": "Mingling mobile app",
	"main": "index.ts",
	"private": true,
	"scripts": {
		"submit": "eas submit --platform ios",
		"version:patch": "npm version patch --no-git-tag-version",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"local": "cross-env APP_ENV=local expo start",
		"dev": "cross-env APP_ENV=development expo start",
		"qa": "cross-env APP_ENV=qa expo start",
		"prod": "cross-env APP_ENV=production expo start",
		"build:dev": "run-s version:patch build:dev:ios build:dev:android",
		"build:qa": "run-s version:patch build:qa:ios build:dev:android",
		"build:prod:all": "run-s version:patch build:prod:ios build:prod:android",
		"prebuild:dev": "cross-env APP_ENV=development expo prebuild && node ../../scripts/prebuild.cjs mobile",
		"prebuild:qa": "cross-env APP_ENV=qa expo prebuild && node ../../scripts/prebuild.cjs mobile",
		"prebuild:prod": "cross-env APP_ENV=production expo prebuild && node ../../scripts/prebuild.cjs mobile",
		"build:dev:ios": "cd ios && pod install && bundle exec fastlane ios build_development",
		"build:qa:ios": "cd ios && pod install && bundle exec fastlane ios beta",
		"build:prod:ios": "cd ios && pod install && bundle exec fastlane ios build",
		"build:dev:android": "cd android && bundle exec fastlane android build_development",
		"build:prod:android": "cd android && bundle exec fastlane android build_and_upload",
		"bundle:ios": "npx expo export --platform ios",
		"bundle:android": "npx expo export --platform android",
		"build:eas:dev:ios": "eas build -p ios --profile development",
		"build:eas:qa:ios": "eas build -p ios --profile qa --auto-submit",
		"build:eas:prod:ios": "eas build -p ios --profile production",
		"build:eas:preview:ios": "eas build -p ios --profile preview",
		"build:local": "eas build -p ios --local",
		"lint": "eslint -c ./.eslintrc.cjs --ignore-path ./.eslintignore \"./**/*.{ts,tsx,cjs}\"",
		"type-check": "tsc --project ./tsconfig.json"
	},
	"dependencies": {
